model device_approval {
  approval_id             Int       @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
  approval_type_id        Int /// @zod.custom.use(z.coerce.number())
  sub_approval_id         Int? /// @zod.custom.use(z.coerce.number().optional())
  requester_id            Int /// @zod.custom.use(z.coerce.number())
  request_at              DateTime  @db.Timestamp(0)
  approver_role_id        Int /// @zod.custom.use(z.coerce.number())
  origin_location_id      Int? /// @zod.custom.use(z.coerce.number().optional())
  destination_location_id Int? /// @zod.custom.use(z.coerce.number().optional())
  approver_id             Int? /// @zod.custom.use(z.coerce.number().optional())
  approve_at              DateTime? @db.Timestamp(0)
  is_approved             Int?      @db.TinyInt /// @zod.custom.use(z.coerce.number().optional())

  @@index([approver_role_id], map: "fk_approval_approver_group1_idx")
  @@index([sub_approval_id], map: "fk_device_approval_device_approval1_idx")
  @@index([approval_type_id], map: "fk_device_approval_device_approval_type1_idx")
  @@index([origin_location_id], map: "fk_device_approval_location1_idx")
  @@index([destination_location_id], map: "fk_device_approval_location2_idx")
  @@index([requester_id], map: "fk_device_approval_user1_idx")
  @@index([approver_id], map: "fk_device_approval_user2_idx")
}

model device_approval_type {
  approval_type_id   Int    @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
