generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

generator zod {
  provider               = "zod-prisma-types"
  output                 = "./zod-schemas"
  addInputTypeValidation = "true"
  createModelTypes       = "true"
  createInputTypes       = "true"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model approval_role {
  role_id       Int    @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
  role_name     String @db.VarChar(45) /// @zod.custom.use(z.string().max(45))
  upper_role_id Int? /// @zod.custom.use(z.coerce.number().optional())

  @@index([upper_role_id], map: "fk_approval_role_approval_role1_idx")
}

model call {
  call_id                  Int       @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
  call_type_id             Int /// @zod.custom.use(z.coerce.number())
  client_branch_id         Int /// @zod.custom.use(z.coerce.number())
  requester_name           String    @db.VarChar(50) /// @zod.custom.use(z.string().max(50))
  requester_num            String    @db.VarChar(20) /// @zod.custom.use(z.string().max(20))
  requester_black_consumer Int       @default(0) @db.TinyInt /// @zod.custom.use(z.coerce.number().default(0))
  device_id                Int? /// @zod.custom.use(z.coerce.number().optional())
  detail                   String    @db.VarChar(200) /// @zod.custom.use(z.string().max(200))
  state                    String    @db.VarChar(45) /// @zod.custom.use(z.string().max(45))
  received_at              DateTime  @db.Timestamp(0)
  receiver_id              Int /// @zod.custom.use(z.coerce.number())
  transferred_at           DateTime? @db.Timestamp(0)
  transferred_dept_id      Int? /// @zod.custom.use(z.coerce.number().optional())
  assigner_id              Int? /// @zod.custom.use(z.coerce.number().optional())
  completed_at             DateTime? @db.Timestamp(0)

  @@index([call_type_id], map: "fk_call_call_type1_idx")
  @@index([client_branch_id], map: "fk_call_client_branch1_idx")
  @@index([transferred_dept_id], map: "fk_call_dept1_idx")
  @@index([device_id], map: "fk_call_device1_idx")
  @@index([receiver_id], map: "fk_call_user1_idx")
  @@index([assigner_id], map: "fk_call_user2_idx")
}

model call_type {
  call_type_id        Int    @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
  call_type_name      String @db.VarChar(50) /// @zod.custom.use(z.string().max(50))
  parent_call_type_id Int? /// @zod.custom.use(z.coerce.number().optional())

  @@index([parent_call_type_id], map: "fk_call_type_call_type1_idx")
}

model client {
  client_id                     Int    @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
  parent_client_id              Int? /// @zod.custom.use(z.coerce.number().optional())
  default_client_branch_rate_id Int /// @zod.custom.use(z.coerce.number())
  client_name                   String @db.VarChar(45) /// @zod.custom.use(z.string().max(45))
  is_active                     Int    @default(1) @db.TinyInt /// @zod.custom.use(z.coerce.number().default(1))

  @@index([parent_client_id], map: "fk_client_client1_idx")
  @@index([default_client_branch_rate_id], map: "fk_client_client_branch_rate1_idx")
}

model client_branch {
  client_branch_id      Int     @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
