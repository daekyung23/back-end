model device {
  device_id              Int      @id @default(autoincrement()) /// @zod.custom.use(z.coerce.number())
  device_model_id        Int /// @zod.custom.use(z.coerce.number())
  owner_dept_id          Int /// @zod.custom.use(z.coerce.number())
