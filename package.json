{
  "name": "crm-system",
  "version": "1.0.0",
  "type": "module",
  "description": "Customer Relationship Management",
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=10.5.2"
  },
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production node -r ./dist/config/alias-paths.js dist/server.js",
    "dev": "cross-env NODE_ENV=development npx tsx watch src/server.ts",
    "prebuild": "npx rimraf dist",
    "build": "npm run prisma:generate && tsc && tsc-alias",
    "watch": "tsc -w",
    "backup": "cross-env NODE_ENV=development node scripts/backup/run-backup.js",
    "restore": "cross-env NODE_ENV=development node scripts/backup/run-restore.js",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push && node scripts/prisma/push-views.js",
    "prisma:generate": "prisma generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.1.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonc-parser": "^3.3.1",
    "mysql2": "^3.11.4",
    "type-fest": "^4.27.0",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@prisma/internals": "^5.1.1",
    "@prisma/sdk": "^4.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "minimist": "^1.2.8",
    "nodemon": "^3.0.2",
    "prisma": "5.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
